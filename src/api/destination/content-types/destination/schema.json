      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Description": {
      "type": "blocks"
    },
    "tours": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tour.tour",
      "inversedBy": "destinations"
    },
    "SEO": {
      "type": "component",
      "component": "shared.metadata",
      "repeatable": true
    }
  }
